{
  "name": "dd-chords-pwa",
  "version": "0.0.9",
  "scripts": {
    "ng": "ng",
    "generate-chords-index": "node generate-chords-index",
    "start": "node server.js",
    "prebuild": "rimraf dist && npm run generate-chords-index",
    "build": "ng build --configuration production",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "ng build",
    "build:watch": "npm run build:dev -- --watch",
    "test": "jest --ci --logHeapUsage",
    "test:dev": "jest --updateSnapshot --bail --runInBand",
    "test:watch": "npm run test:dev -- --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "name": "dd-chords-pwa",
    "coverageDirectory": "dist/coverage",
    "collectCoverageFrom": [
      "src/app/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".module.ts",
      "index.ts",
      ".stories.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".stories.ts",
      "test.ts"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@angular/service-worker": "~12.2.17",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "dd-rxjs": "^1.5.0",
    "rxjs": "~6.6.0",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "@types/web-bluetooth": "0.0.9",
    "codelyzer": "^6.0.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^10.5.3",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-mockery": "^1.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
